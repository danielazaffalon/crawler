{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
    },
    "source": {
      "include": ["index.js", "log.js", "readLine.js", "scraping.js", "storageService.js", "config.js"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "encoding": "utf8",
      "destination": "./docs/",
      "recurse": true,
      "verbose": true
    }
  }